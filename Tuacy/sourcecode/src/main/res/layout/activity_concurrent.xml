<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <Button
        android:id="@+id/btn_test_thread"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Thread"/>

    <Button
        android:id="@+id/btn_test_runnable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Runnable"/>

    <Button
        android:id="@+id/btn_test_future_task"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FutureTask"/>

    <Button
        android:id="@+id/btn_test_completionService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CompletionService"/>




</LinearLayout>
